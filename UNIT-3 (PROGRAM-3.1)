import numpy as np
print("Help on np.add function:\n")
help(np.add)
arr=np.array([1,2,3,4,5])
print("Array1",arr)
arr2=np.array([1,0,2,3])
print("Array:",arr2)
print("Non of the array element is zero",np.all(arr))
print("None of the array element is zero",np.all(arr2))
x=np.array([1,2,3,4,5])
y=np.array([2,2,2,2,2])
print("\nArray x:",x)
print("\nArray y:",y)
print("x>y",np.greater(x,y))
print("x>=y",np.greater_equal(x,y))
print("x<y",np.less(x,y))
print("x<=y",np.less_equal(x,y))
print("x=y",np.equal(x,y))
print(np.allclose(x,y))
z=np.zeros((2,2))
o=np.ones((2,3))
l=np.linspace(0,1,5)
print("\nZeros array:\n",z)
print("\nOnes array:\n",o)
print("\nLinspace array:\n",l)
print("convert array to list",l.tolist())
